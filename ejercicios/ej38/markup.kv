<MyLayout>
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        Label:
            markup: True
            font_size: 32
            text: "This [sub]is[/sub] [color=#A12C3C][b]Bold[/b][/color] [font=times]Text[/font]"
        
        Label:
            markup: True
            font_size: 32
            text: "This [sup]is[/sup] [size=150][color=#A12C3C][i]Italics[/i][/color][/size] Text"
        
        Button:
            markup: True
            font_size: 32
            text: "[u]Click[/u] [s]me[/s]!"   
            